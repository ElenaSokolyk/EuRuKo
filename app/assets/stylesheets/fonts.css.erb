@font-face {
	font-family: 'Nexa Bold';
	src: url('/fonts/Nexa Bold.eot');
  src: url('/fonts/Nexa Bold.eot') format('embedded-opentype'),
       url('/fonts/Nexa Bold.woff') format('woff'),
       url('/fonts/Nexa Bold.otf') format('opentype'),
       url('/fonts/Nexa Bold.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
  font-family:'Sansation_Regular';
  src: url('/fonts/Sansation_Regular.eot');
  src: url('/fonts/Sansation_Regular.eot') format('embedded-opentype'),
       url('/fonts/Sansation_Regular.woff') format('woff'),
       url('/fonts/Sansation_Regular.otf') format('opentype'),
       url('/fonts/Sansation_Regular.svg') format('svg');
  font-weight: normal;
  font-style: normal;
}